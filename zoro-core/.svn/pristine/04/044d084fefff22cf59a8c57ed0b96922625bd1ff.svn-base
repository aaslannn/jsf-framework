body {
	background: #fff url("#{resource['img/page/background.png']}") repeat-x
		top;
	font: normal 12px Arial;
	margin: 0;
	padding: 4px 0 0 0;
	color: #2E6E9E;
}

.ui-widget-content a {
   color: #2E6E9E;
}

.ui-widget-content {
   color: #2E6E9E;
}

.ui-widget input, .ui-widget select, .ui-widget textarea, .ui-widget button {
    color: #2E6E9E;
}

div.header {
	height: 100px;
	margin: 0 auto;
	position: relative;
	width: 1150px;
}

div.logo {
	height: 33px;
	left: 10px;
	position: absolute;
	top: 25px;
	width: 156px;
}

div.menu {
	position: absolute;
	right: 0;
	top: 41px;
}

div.userInfo {
	padding-right: 12px;	
}

div#main {
	background-color: #fff;
	border-top: solid 3px #9fa3ac;
	display: block;
	margin: 0 auto;
	padding: 10px 20px;
	position: relative;
	width: 1110px;
}

div#main:after {
	content: ".";
	display: block;
	height: 0;
	clear: both;
	visibility: hidden;
}

div#main div.left-shadow {
	background: url("#{resource['img/page/left-shadow.png']}") no-repeat top
		right;
	height: 154px;
	left: -31px;
	position: absolute;
	top: 0;
	width: 31px;
}

div#main div.right-shadow {
	background: url("#{resource['img/right-shadow.png']}") no-repeat top
		left;
	position: absolute;
	right: -31px;
	top: 0;
	width: 31px;
}

div#main div#content {
	float: left;
	width: 100%;
	min-height: 400px;
	padding-right: 16px;
}

div#main div#sidebar {
	float: right;
	width: 230px;
}

div#main div#sidebar div.sidebar-panel {
	background: url("#{resource['img/page/sidebar-panel-back.png']}")
		repeat-x bottom;
	border: solid 1px #cbced6;
	padding: 9px 11px;
	position: relative;
}

div#main div#sidebar div.sidebar-panel p,div#main div#sidebar div.sidebar-panel ul
	{
	margin: 6px 0;
}

div#main div#sidebar div.sidebar-panel ul li {
	margin-bottom: 6px;
}

div#main div#sidebar div.sidebar-panel div.sidebar-panel-shadow {
	background: url("#{resource['img/page/sidebar-panel-shadow.png']}")
		no-repeat center;
	bottom: -12px;
	left: 0;
	height: 12px;
	position: absolute;
	width: 100%;
}

div#footer {
	background: url("#{resource['primefaces-redmond/images/ui-bg_gloss-wave_55_5c9ccc_500x100.png']}") repeat-x scroll 50% 50% #5C9CCC;
	color: #2E6E9E;
	font-size: 12px;
	height: 50px;
	position: relative;
}

div#footer a {
	color: #2E6E9E;
	margin: 0 2px;
}

div#footer a:hover {
	text-decoration: underline;
}

div#footer div.holder {
	margin: 0 auto;
	padding-top: 10px;
	position: relative;
	width: 1000px;
}

div#footer div.holder div.left {
	left: 5px;
	position: absolute;
}

div#footer div.holder div.right {
	position: absolute;
	right: 5px;
}

a {
	color: #003399;
	outline: none;
	text-decoration: none;
}

a:hover {
	color: #d70f11;
	text-decoration: underline;
}

h1,h2 {
	color: #1f335f;
	font-size: 20px;
	font-weight: normal;
	margin-bottom: 10px;
}

h3 {
	color: #444;
	font-size: 16px;
	font-weight: normal;
	margin: -13px 0 10px 30px;
}

h4 {
	color: #1f335f;
	font-size: 16px;
	font-weight: normal;
	margin-bottom: 5px;
}

label {
	margin: 4px 0;
}

input[type=password] {
	margin: 3px 0 6px 0;
}

input[type=button] {
	margin: 5px 0;
}

ul.mtList {
	padding-left: 15px;
}

ul.mtList li {
	list-style-image: url("#{resource['img/page/bullet.png']}");
	margin-bottom: 5px;
}

.bullet {
	background: url("#{resource['img/page/bullet.png']}") no-repeat left
		center;
	display: inline-block;
	height: 15px;
	width: 9px;
}

.light {
	color: #777;
}

button {
	margin: 5px;
}

.editorPanelGrid
{
	margin-bottom: 1em;
}